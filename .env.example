# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Environment
NODE_ENV=development

# Optional: Feature Flags
VITE_FEATURE_FLAGS_ENABLED=true

# Social Authentication Configuration
# Force Real OAuth for All Providers
# Set to 'true' to attempt real OAuth for all providers (LinkedIn, Spotify, etc.)
# even if they typically require manual setup in Supabase
VITE_FORCE_REAL_OAUTH=false

# Individual Provider Override (future enhancement)
# VITE_GOOGLE_OAUTH_ENABLED=true
# VITE_FACEBOOK_OAUTH_ENABLED=true
# VITE_LINKEDIN_OAUTH_ENABLED=false
# VITE_SPOTIFY_OAUTH_ENABLED=false

# Optional: PWA Configuration
VITE_PWA_ENABLED=true

# Stripe Configuration (for payments and subscriptions)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-publishable-key
STRIPE_SECRET_KEY=sk_test_your-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Supabase Edge Functions Environment Variables
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
